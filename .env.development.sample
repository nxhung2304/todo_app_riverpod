# Development Environment
API_BASE_URL=https://api-dev.yourapp.com
API_TIMEOUT_CONNECT=30
API_TIMEOUT_RECEIVE=30
API_ENABLE_LOGGING=true
API_MAX_RETRIES=3
API_CACHE_ENABLED=true
API_CACHE_DURATION_MINUTES=5

# Authentication
AUTH_TOKEN_REFRESH_THRESHOLD_MINUTES=5
AUTH_CLIENT_ID=dev_client_id
AUTH_CLIENT_SECRET=dev_client_secret

# Debug Settings
DEBUG_MODE=true
LOG_LEVEL=debug
